<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Hangouts</display-name>
  <welcome-file-list>
    <welcome-file>welcome.html</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>com.teamchat.client.utils.TeamchatContextListener</listener-class>
  </listener>
  <context-param>
    <param-name>bot-email</param-name>
    <param-value></param-value>
  </context-param>
  <context-param>
    <param-name>bot-password</param-name>
    <param-value></param-value>
  </context-param>
  <context-param>
    <param-name>bot-class</param-name>
    <param-value>com.teamchat.integration.hangouts.HangoutsBot</param-value>
  </context-param>
  <context-param>
    <param-name>db-host</param-name>
    <param-value>localhost</param-value>
  </context-param>
  <context-param>
    <param-name>db-name</param-name>
    <param-value></param-value>
  </context-param>
  <context-param>
    <param-name>db-user</param-name>
    <param-value></param-value>
  </context-param>
  <context-param>
    <param-name>db-password</param-name>
    <param-value></param-value>
  </context-param>
  <context-param>
    <param-name>log-location</param-name>
    <param-value>./logs</param-value>
  </context-param>
  <context-param>
    <param-name>log-level</param-name>
    <param-value>ALL</param-value>
  </context-param>
  <context-param>
    <param-name>log-filename</param-name>
    <param-value>hangouts</param-value>
  </context-param>
  <context-param>
    <param-name>threading</param-name>
    <param-value>SINGLE</param-value>
  </context-param>
</web-app>
