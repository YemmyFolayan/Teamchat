package com.pivotal.bot;



public class Pivotal_basics
{
	private String email,access_token;

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getAccess_token() {
		return access_token;
	}

	public void setAccess_token(String access_token) {
		this.access_token = access_token;
	}

	public Pivotal_basics(){
		
	}
	
	public Hipchat_basiccheckbot(String email,String access_token) {
		super();
		this.email = email;
		this.access_token = access_token;
	}
}